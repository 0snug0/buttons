{
  "compilerOptions": {
    "baseUrl": ".",
    "inlineSourceMap": true,
    "inlineSources": true,
    "module": "ESNext",
    "target": "ES2020",
    "allowJs": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "lib": ["webworker", "DOM", "ES5", "ES6", "ES7", "es2019"]
  },
  "include": ["src/*.ts", "src/**/.ts"]
}
